<h2>Show User</h2>

<ul>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @user.name %>
  </li>

</ul>

<%= if @current_user == @user do %>
  <span><%= link "Back to Feed", to: page_path(@conn, :feed) %></span>
<% else %>
  <span><%= link "Log in", to: page_path(@conn, :index) %></span>
<% end %>
